<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vsu.csf.procat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ProCatApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProCat"
        android:usesCleartextTraffic="true">

        <activity
            android:name=".ui.LauncherActivity"
            android:noHistory="true"
            android:theme="@style/noAnimTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.rentinventorylist.RentInventoryListActivity" />
        <activity android:name=".ui.rentstationslist.RentStationsListActivity" />
        <activity android:name=".ui.ProfileActivity" />

    </application>

</manifest>